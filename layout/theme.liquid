{% comment %}
  Add the page template as a class for easy page or template specific styling.
{% endcomment %}
<body id="{{ page_title | handle }}" class=" {% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}" >
  <div class="wrapper-container">	
	{% section 'header' %}
    {% section 'navigation' %}
    {% section 'horizental-menu' %}
    <div class="quick-view"></div>
    {% if template contains 'index' or template contains 'contact' %}
    {% else %}
    	{% include 'breadcrumb' %}
    {% endif%}
    <main class="main-content {% if template contains 'contact' %}{% else %}container{% endif %}" role="main">
      {{ content_for_layout }}
    </main>
    

    
    {% if template contains 'collection' or template contains 'product'  %}
    {% unless template contains 'list-collection' %}
    	{% section 'recently-viewed-products' %}
    {% endunless %}
    {% endif %}

     {% section 'brands' %}
     {% section 'footer' %}

    <script src="//cdn.codeblackbelt.com/js/modules/frequently-bought-together/main.min.js?shop=vxdas-official-store.myshopify.com" defer></script>

    

    
	
    {% if settings.enable_quick_view %}
      {% include 'quick-view-template' %}
    {% endif %}

    {% include 'footer_scripts' %}
    {% include 'newsletter' %}
    <div id="back-top" style="display: none;"><a class="nav_up" href="#top">Back to top</a></div>
  </div>

    <div class="overlay-mn"></div>
  
  {% if settings.display_recently_viewed_products %}
  <!-- Begin Recently Viewed Products -->
  {{ '//ajax.aspnetcdn.com/ajax/jquery.templates/beta1/jquery.tmpl.min.js' | script_tag }}
  {{ 'jquery.products.min.js' | asset_url | script_tag }}
  {% if template contains 'product' %}
  <script>
  Shopify.Products.recordRecentlyViewed();
  </script>
  {% endif %}
  <!-- End Recently Viewed Products -->
  {% endif %}
  {% include 'sweettooth-widgets' %}
  
  {{ 'wishlish.js' | asset_url | script_tag }}
    <script>
      (function() {
        Wishlist.init();
      }());
    </script>
   {% include 'search-autocomplete' %}
