      box-shadow: 1px 0 5px rgba(0, 0, 0, 0.08);
      border-radius:3px;
    }
    
    .sidebar-links{
      border:none;
    }
    
    
    .widget-title h3{
      border-top:1px solid #d7d7d7;
        border-bottom:0;
    }
    
    .sidebar-custom{
      margin-top:0;
        border-radius:0;
        border:none;
    }
  
    .widget-featured-product{
        @extend .sidebar-custom;
        
      .products-grid{
        .action{
          .btn{
            padding: 9px 10px 7px 10px;
            font-size:12px
          }
        }
      }

      .widget-product{
        padding: 20px;
      }
    }
    
    .sidebar-cms-custom{
       @extend .sidebar-custom;
    }
    
    .sidebar-banner{
      margin-top:0;
        display:none;
    }
    
    
    
    .sidebar{
      display:none;
    }
    
    .widget-content{
      display:none;
    }
    
    .widget-title{
      h3{
          i{
              display:block!important;
            }
        }
    }
    
    .sidebar-mb{
      display:block;
    }
    
    .refined-widgets{
      margin-top:0;
      margin-bottom: 20px;
      
      .widget-title{
        h3{
          padding:13px 15px;
        }
      }
      
      ul{
      	padding:5px 15px;
        margin-bottom:0;
      }
      
      .clear-all{
      	margin-left:15px;
      }
    }
    
  }
  
  .col-main{
    -ms-flex: 0 0 100%;
    flex: 0 0 100%;
    max-width: 100%;
  }
  
  .toolbar-bottom{
    flex-direction: column-reverse;
    -webkit-flex-direction: column-reverse;

    li{
      a{
        display: inline-block;
        margin-bottom: 15px;
      }
    }
  }
  
  .recently-viewed-products{
    padding-bottom:0px!important;
  }
  
  .sidebar-blog{
    
    .widget-categories{
      border:none;
      margin-bottom: 0;
      border-radius: 0;
    }
    
    .widget-recent-articles{
      @extend .widget-categories;
    }
    
    .search-bar-blog{
      padding: 15px;
      margin-bottom: 0;
    }
  }
  
}
@media (min-width:1024px){
  .page-sidebar{
    .sidebar{
      display:block!important;
    }
  }
  
  .sidebar-blog{
    .widget-title{
    	h3{
        	i{
            	display:none;
            }
        }
    }
  }
}

@media (max-width:1023px) and (min-width:768px){
  .footer-newsletter{
  	.block-content{
      .input-group{
      	display:block;
      }
      .input-group-field{
      	margin-bottom:10px;
      }
    }
    
    .newsletter{
    	.btn{
        	margin-left:0!important;
        }
    }
  }

}

@media (max-width:1024px) and (min-width:768px){
    
  .footer_address{
    padding-left: 0;
  }
}

@media (max-width:1450px) and (min-width:1200px){
  
  .nav-bar-mobile{
    .menu-contain{
      -ms-flex: 0 0 27%;
      flex: 0 0 27%;
      max-width: 27%;
    }

    .nav-search {
      -ms-flex: 0 0 73%;
      flex: 0 0 73%;
      max-width: 73%;
    }
    
    .nav-bar{
      .site-nav{
        .mega-menu{
          .site-nav-dropdown{
            min-width:710px;
          }
        }
      }
    }
  }
  
